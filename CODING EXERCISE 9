package myPractice;

import java.util.Scanner;

class Exercise9 {

	public static void main(String[] args) {
		Scanner in=new Scanner(System.in);
		
        int mark;
		
		System.out.print("Enter your percentage marks : ");
		mark=in.nextInt();
		
		switch(mark /10) {
		case(9) : System.out.print("Excellent: Grade A");
		break;
		
		case(8) : System.out.print(" Very Good: Grade B ");
		break;
		
		case(7) : System.out.print(" Good: Grade C ");
		break;
		
		case(6) : System.out.print(" Good: Grade D ");
		break;
		
		case(5) : System.out.print(" Good: Grade E ");
		break;
		
		case(4) : System.out.print(" Just Passed: Grade F ");
		break;
		
		default : System.out.print("Failed!");
		
		
		}

	}

}
